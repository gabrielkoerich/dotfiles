#!/usr/bin/env bash

# Install Fonts
cp ../fonts/* /Library/Fonts/
echo "Fonts installed.";

# Install font tools.
brew install woff2

# clone
git clone https://github.com/powerline/fonts.git powerline-fonts --depth=1
./powerline-fonts/install.sh
rm -rf powerline-fonts

echo "Powerline Fonts installed.";
